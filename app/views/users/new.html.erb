<%= form_with model: @user do |f| %>

    <%= f.label :uid, 'UID' %>
    <%= f.text_field :uid %>
    <%= f.label :password, 'パスワード' %>
    <%= f.password_field :password %>
  　<%= f.label :password_confirmation, 'パスワード(確認)' %>
　　<%= f.text_field :password_confirmation %>

    <%= f.submit '登録' %>
  
<% end %>
